{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "linebreak-style": "off",
    "no-underscore-dangle": "off",
    "no-console": "off",
    "import/extensions": "off",
    "func-names": "off"
  }
}
